<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="io.dbc.github.sninventory.controller.MajorController">
    <VBox alignment="CENTER" layoutY="-2.0" prefHeight="400.0" prefWidth="600.0">
        <Button fx:id="expiredProductButton" mnemonicParsing="false" onAction="#onExpiredProductButtonClick"
                prefHeight="31.0" prefWidth="153.0" text="Expired Product">
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="nearExpiryProductButton" mnemonicParsing="false" onAction="#onNearExpiryProductButtonClick"
                text="Near Expiry Product">
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="dumpStockButton" mnemonicParsing="false" onAction="#onDumpStockButtonClick" prefHeight="31.0"
                prefWidth="157.0" text="Dump Stock">
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
        </Button>
    </VBox>
</AnchorPane>
