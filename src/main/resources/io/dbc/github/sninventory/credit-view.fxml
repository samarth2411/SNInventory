<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="650.0"
            xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="io.dbc.github.sninventory.controller.CreditController">
    <VBox alignment="CENTER" prefHeight="374.0" prefWidth="650.0">
        <Button fx:id="creditButton" alignment="CENTER" mnemonicParsing="false" onAction="#onCreditButtonClick"
                prefHeight="35.0" prefWidth="155.0" text="Credit">
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
            <font>
                <Font size="17.0"/>
            </font>
        </Button>
        <Button fx:id="outstandingButton" alignment="CENTER" mnemonicParsing="false"
                onAction="#onOutstandingButtonClick" prefHeight="35.0" prefWidth="155.0" text="Outstanding">
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
            <font>
                <Font size="16.0"/>
            </font>
        </Button>
    </VBox>
    <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" layoutY="373.0" mnemonicParsing="false"
            onAction="#onBackButtonClick" prefHeight="26.0" prefWidth="53.0" text="&lt; Back"/>
    <Separator layoutY="369.0" prefHeight="8.0" prefWidth="650.0"/>
</AnchorPane>