<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="io.dbc.github.sninventory.controller.CreditController">

    <children>
        <VBox alignment="CENTER" layoutX="-1.0" layoutY="1.0" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Button fx:id="creditButton" mnemonicParsing="false" prefHeight="41.0" prefWidth="241.0" text="Credit">
                    <font>
                        <Font size="20.0"/>
                    </font>
                    <VBox.margin>
                        <Insets bottom="15.0"/>
                    </VBox.margin>
                    <padding>
                        <Insets left="4.0" right="4.0"/>
                    </padding>
                </Button>
                <Button fx:id="outstandingButton" mnemonicParsing="false" prefHeight="44.0" prefWidth="243.0"
                        text="Outstanding">
                    <font>
                        <Font size="20.0"/>
                    </font>
                    <VBox.margin>
                        <Insets bottom="20.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>

</AnchorPane>
