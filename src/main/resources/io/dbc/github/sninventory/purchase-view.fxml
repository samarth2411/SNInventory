<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="io.dbc.github.sninventory.controller.PurchaseController">
    <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
        <Button fx:id="addNewPurchaseButton" alignment="CENTER" mnemonicParsing="false"
                onAction="#onAddNewPurchaseButtonClick" prefHeight="35.0" prefWidth="155.0" text="Add New Purchase">
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="showPreviousPurchaseButton" alignment="CENTER" mnemonicParsing="false"
                onAction="#onshowPreviousPurchaseButtonClick" prefHeight="35.0" prefWidth="155.0"
                text="Show Previous Purchase">
            <VBox.margin>
                <Insets top="15.0"/>
            </VBox.margin>
        </Button>
    </VBox>
</AnchorPane>
