<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.dbc.github.sninventory.controller.PurchaseController">
    <VBox alignment="CENTER" prefHeight="373.0" prefWidth="650.0">
        <Button fx:id="addNewPurchaseButton" alignment="CENTER" mnemonicParsing="false" onAction="#onAddNewPurchaseButtonClick" prefHeight="37.0" prefWidth="171.0" text="Add New Purchase">
            <VBox.margin>
                <Insets bottom="15.0" />
            </VBox.margin>
         <font>
            <Font size="14.0" />
         </font>
        </Button>
        <Button fx:id="showPreviousPurchaseButton" alignment="CENTER" mnemonicParsing="false" onAction="#onshowPreviousPurchaseButtonClick" prefHeight="38.0" prefWidth="175.0" text="Show Previous Purchase">
            <VBox.margin>
                <Insets top="15.0" />
            </VBox.margin>
         <font>
            <Font size="13.0" />
         </font>
        </Button>
    </VBox>
    <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" layoutY="373.0" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="26.0" prefWidth="53.0" text="&lt; Back" />
    <Separator layoutY="369.0" prefHeight="8.0" prefWidth="650.0" />
</AnchorPane>
