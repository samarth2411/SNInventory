<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.dbc.github.sninventory.controller.LoginController">
    <VBox alignment="CENTER" layoutX="1.0" layoutY="1.0" prefHeight="400.0" prefWidth="650.0">
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <Label text="username">
                <font>
                    <Font size="27.0" />
                </font>
            </Label>
            <TextField fx:id="usernameTextField" prefHeight="26.0" prefWidth="150.0">
                <HBox.margin>
                    <Insets left="8.0" />
                </HBox.margin>
            </TextField>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <Label text="password">
                <font>
                    <Font size="27.0" />
                </font>
            </Label>
            <PasswordField fx:id="passwordField">
                <HBox.margin>
                    <Insets left="8.0" />
                </HBox.margin>
            </PasswordField>
        </HBox>
        <Button fx:id="loginButton" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="29.0" prefWidth="86.0" text="Login">
         <font>
            <Font size="14.0" />
         </font></Button>
    </VBox>
</AnchorPane>
