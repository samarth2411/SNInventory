<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.dbc.github.sninventory.controller.SalesController">
    <VBox alignment="CENTER" prefHeight="371.0" prefWidth="650.0">
        <Button fx:id="addNewSaleButton" alignment="CENTER" mnemonicParsing="false" onAction="#onAddNewSaleButtonClick" prefHeight="35.0" prefWidth="155.0" text="Add New Sale">
            <VBox.margin>
                <Insets bottom="15.0" />
            </VBox.margin>
         <font>
            <Font size="14.0" />
         </font>
        </Button>
        <Button fx:id="showPreviousSalesButton" alignment="CENTER" mnemonicParsing="false" onAction="#onShowPreviousSalesButtonClick" prefHeight="35.0" prefWidth="155.0" text="Show Previous Sale">
            <VBox.margin>
                <Insets top="15.0" />
            </VBox.margin>
         <font>
            <Font size="13.0" />
         </font>
        </Button>
    </VBox>
    <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" layoutY="373.0" mnemonicParsing="false" onAction="#onBackButtonClick" prefHeight="26.0" prefWidth="53.0" text="&lt; Back" />
    <Separator layoutY="369.0" prefHeight="8.0" prefWidth="650.0" />
</AnchorPane>
