<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="io.dbc.github.sninventory.controller.SalesController">

    <VBox alignment="CENTER" layoutX="-1.0" layoutY="1.0" prefHeight="400.0" prefWidth="600.0">
        <Button fx:id="addNewSalesButton" mnemonicParsing="false" onAction="#onaddNewSalesButtonClick"
                prefHeight="41.0" prefWidth="241.0" text="Add new Sales">
            <font>
                <Font size="20.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="15.0"/>
            </VBox.margin>
            <padding>
                <Insets left="4.0" right="4.0"/>
            </padding>
        </Button>
        <Button fx:id="showPreviousSalesButton" mnemonicParsing="false" onAction="#onshowPreviousSalesButtonClick"
                prefHeight="44.0" prefWidth="243.0" text="Show Previous Sales">
            <font>
                <Font size="20.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="20.0"/>
            </VBox.margin>
        </Button>
    </VBox>
</AnchorPane>
