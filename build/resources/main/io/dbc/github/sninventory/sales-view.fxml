<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="io.dbc.github.sninventory.controller.SalesController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Button fx:id="addNewSaleButton" alignment="CENTER" mnemonicParsing="false" prefHeight="35.0"
                        prefWidth="155.0" text="Add New Sale">
                    <VBox.margin>
                        <Insets bottom="15.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="showPreviousSaleButton" alignment="CENTER" mnemonicParsing="false" prefHeight="35.0"
                        prefWidth="155.0" text="Show Previous Sale">
                    <VBox.margin>
                        <Insets top="15.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
