<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="io.dbc.github.sninventory.controller.CreditController">
    <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
        <Button fx:id="creditButton" alignment="CENTER" mnemonicParsing="false" onAction="#onCreditButtonClick"
                prefHeight="35.0" prefWidth="155.0" text="Credit">
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="outstandingButton" alignment="CENTER" mnemonicParsing="false"
                onAction="#onOutstandingButtonClick" prefHeight="35.0" prefWidth="155.0" text="Outstanding">
            <VBox.margin>
                <Insets bottom="15.0" top="15.0"/>
            </VBox.margin>
        </Button>
    </VBox>
</AnchorPane>
