    <TableView fx:id="currentStockTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="productNameColumn" prefWidth="199.0" text="Product Name" />
            <TableColumn fx:id="quantityColumn" prefWidth="213.0" text="Quantity" />
            <TableColumn fx:id="priceColumn" prefWidth="187.0" text="Price" />
        </columns>
    </TableView>
</AnchorPane>
