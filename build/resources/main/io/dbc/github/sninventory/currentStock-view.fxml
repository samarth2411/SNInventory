<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="650.0"
            xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="io.dbc.github.sninventory.controller.CurrentStockController">
    <TableView fx:id="currentStockTable" layoutY="1.0" prefHeight="372.0" prefWidth="650.0">
        <columns>
            <TableColumn fx:id="productNameColumn" prefWidth="255.60000610351562" text="Product Name"/>
            <TableColumn fx:id="quantityColumn" prefWidth="210.80001831054688" text="Quantity "/>
            <TableColumn fx:id="priceColumn" minWidth="0.0" prefWidth="181.20001220703125" text="Price"/>
        </columns>
    </TableView>
    <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" layoutY="373.0" mnemonicParsing="false"
            onAction="#onBackButtonClick" prefHeight="26.0" prefWidth="53.0" text="&lt; Back"/>
    <Separator layoutY="369.0" prefHeight="8.0" prefWidth="650.0"/>
</AnchorPane>