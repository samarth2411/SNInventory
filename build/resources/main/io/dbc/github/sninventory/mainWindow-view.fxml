<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/16"
            fx:controller="io.dbc.github.sninventory.controller.MainWindowController">
    <Button fx:id="currentStockButton" layoutX="148.0" layoutY="97.0" mnemonicParsing="false"
            onAction="#onCurrentStockButtonClick" prefHeight="26.0" prefWidth="98.0" text="Current Stock "/>
    <Button fx:id="productDetailsButton" layoutX="338.0" layoutY="97.0" mnemonicParsing="false"
            onAction="#onProductDetailsButtonClick" prefHeight="26.0" prefWidth="98.0" text="Product Details"/>
    <Button fx:id="purchaseButton" layoutX="148.0" layoutY="164.0" mnemonicParsing="false"
            onAction="#onPurchaseButtonClick" prefHeight="26.0" prefWidth="98.0" text="Purchase"/>
    <Button fx:id="salesButton" layoutX="340.0" layoutY="164.0" mnemonicParsing="false" onAction="#onSalesButtonClick"
            prefHeight="26.0" prefWidth="98.0" text="Sales"/>
    <Button fx:id="creditButton" layoutX="148.0" layoutY="232.0" mnemonicParsing="false"
            onAction="#onCreditButtonClick" prefHeight="26.0" prefWidth="98.0" text="Credit"/>
    <Button fx:id="debitButton" layoutX="340.0" layoutY="232.0" mnemonicParsing="false" onAction="#onDebitButtonClick"
            prefHeight="26.0" prefWidth="98.0" text="Debit"/>
    <Button fx:id="profitButton" layoutX="148.0" layoutY="297.0" mnemonicParsing="false"
            onAction="#onProfitButtonClick" prefHeight="26.0" prefWidth="98.0" text="Profit"/>
    <Button fx:id="majorButton" layoutX="340.0" layoutY="297.0" mnemonicParsing="false" onAction="#onMajorButtonClick"
            prefHeight="26.0" prefWidth="98.0" text="Major"/>
</AnchorPane>
